{
  "name": "jquery-range-slider",
  "version": "1.0.0",
  "description": "The second task for fsd",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --mode development --config ./webpack.config.js --progress --color --display-error-details",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production --config ./webpack.config.js  --progress  --color --display-error-details --bail",
    "clean": "npm cache clear && rimraf -- dist",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --mode development --config ./webpack.config.js --hot --inline --progress --watch --open",
    "server:prod": "cross-env NODE_ENV=production webpack-dev-server  --mode production --config ./webpack.config.js --port 3000 --host 0.0.0.0 --hot --inline --progress  --watch --open --content-base dist/",
    "start": "npm run server:dev",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Res01uti0n/Jquery-range-slider-plugin.git"
  },
  "keywords": [
    "jquery",
    "typescript"
  ],
  "author": "resolution",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Res01uti0n/Jquery-range-slider-plugin/issues"
  },
  "homepage": "https://github.com/Res01uti0n/Jquery-range-slider-plugin#readme",
  "dependencies": {
    "@types/jasmine": "^3.5.11",
    "@types/jquery": "^3.5.0",
    "@types/jsdom": "^16.2.3",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "ejs-loader": "^0.5.0",
    "eslint": "^7.3.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "jquery": "^3.5.1",
    "jsdom": "^16.2.2",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "tslint": "^6.1.2",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
